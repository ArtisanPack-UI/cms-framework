{
  "name": "artisanpack-ui/cms-framework",
  "description": "Adds in the back end support for building a CMS with any front end framework.",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "ArtisanPackUI\\CMSFramework\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jacob Martella",
      "email": "me@jacobmartella.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": ">=5.3"
  },
  "require-dev": {
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-laravel": "^3.1"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ArtisanPackUI\\CMSFramework\\CMSFrameworkServiceProvider"
      ],
      "aliases": {
        "Package": "CMSFramework"
      }
    }
  }
}
